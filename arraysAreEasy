#include <iostream>





int oneDarray[2] = { 0,1 };




int twoDarray[3][3] = { 
						{1,2,3},
						{4,5,6},
						{7,8,9}
					  };



int threeDarray[3][3][3] = {
							{ {1,2,3},{10,11,12},{14,15,16} },
							{ {4,5,6},{13,14,15},{17,18,19} },
							{ {7,8,9},{16,17,18},{20,21,22} },
						   };


int main()
{


	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			for (int k = 0; k < 3; k++)
			{
				std::cout << "threeDarray[" << i << "][" << j << "][" << k << "] = " << threeDarray[i][j][k] << std::endl;
			}
		}
	}
}
